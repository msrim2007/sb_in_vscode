<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.demo.web.mapper.NoticeMapper">
    <select id="selectAllNoticeList" resultType="noticeDTO">
        SELECT 
            NOTICE_SEQNO,
            NOTICE_TITLE,
            NOTICE_VIEW_CNT,
            NOTICE_LIKE_CNT,
            NOTICE_REG_DTTM,
            NOTICE_REG_USER_ID
        FROM NOTICE
    </select>
</mapper>